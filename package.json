{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "Dashboard",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack/dev.config.js --host 0.0.0.0 --port 8080 --content-base public/ --history-api-fallback --hot --inline --colors",
    "clean": "rm -rf ./dist",
    "check": "npm run styles && npm run eslint",
    "compile": "mkdir -p ./dist && rsync -av ./public/* ./dist/ --exclude index.html && webpack --config webpack/dev.config.js",
    "build": "npm run check && npm run clean && npm run test && npm run compile",
    "build:serve": "static dist",
    "test": "mocha --require ./test/test_helper.js 'test/**/*-test.js'",
    "styles": "eclint check './src/**/*.js'",
    "eslint": "eslint src",
    "eslint:fix": "eslint --fix src",
    "eslint:config": "eslint --print-config src",
    "uninstall": "npm uninstall `ls -1 node_modules | tr '/\n' ' '`"
  },
  "author": "",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "devDependencies": {
    "animate.css": "^3.4.0",
    "autoprefixer": "^6.3.1",
    "babel-core": "^6.1.21",
    "babel-loader": "^6.1.0",
    "babel-polyfill": "^6.1.19",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "babel-register": "^6.9.0",
    "bootstrap": "^3.3.7",
    "eclint": "^1.1.5",
    "eslint": "^2.2.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^5.1.1",
    "eslint-plugin-standard": "^1.3.2",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.17.0",
    "moment": "^2.13.0",
    "react": "^15.0.0",
    "react-hot-loader": "^1.3.0",
    "react-dom": "^15.0.0",
    "react-redux": "^4.0.0",
    "react-router": "^2.4.0",
    "redux": "^3.0.4",
    "redux-form": "^5.2.3",
    "redux-logger": "^2.0.4",
    "redux-thunk": "^2.1.0",
    "webpack": "^1.12.6",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bootstrap": "^3.3.7",
    "normalizr": "^2.0.0",
    "react-router-redux": "^4.0.8",
    "react-select": "^0.9.1",
    "style-loader": "^1.2.1"
  }
}
